    function downloadJSAtOnload(scriptPaths) {
        scriptPaths.forEach(path => {
            let element = document.createElement("script");
            element.src = path;
            document.body.append(element);
        });
    }
    setTimeout(() => {
        downloadJSAtOnload([
            'https://www.google.com/recaptcha/api.js'
        ]);
    }, 10000);
</script>
