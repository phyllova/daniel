function handledropdowngroup (ev) {
  const nodes = document.querySelectorAll('.dropdowngroup');
  nodes.forEach(function(node) { 
    if (node !== ev.target) {
      node.checked = false;
    }
  })
}

document.addEventListener("DOMContentLoaded", () => {
  const nodes = document.querySelectorAll('.dropdowngroup');
  nodes.forEach(function(node) {
    node.addEventListener('change', handledropdowngroup)
  })
});
</script><script data-rh="true">
function handlemenugroup (ev) {
  const nodes = document.querySelectorAll('.menugroup');
  nodes.forEach(function(node) { 
    if (node !== ev.target) {
      node.checked = false;
    }
  })
}

document.addEventListener("DOMContentLoaded", () => {
  const nodes = document.querySelectorAll('.menugroup');
  nodes.forEach(function(node) {
    node.addEventListener('change', handlemenugroup)
