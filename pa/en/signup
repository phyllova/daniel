	let countryCodeList = $('#countryCodeList');
	let countryCodeList1 = $('#countryCodeList1');
	let countryCodeList2 = $('#countryCodeList2');
	let countryCodeList3 = $('#countryCodeList3');
	let signupPhoneNumber = $('#signup-phone-number');
	let signupPhoneNumber2 = $('#signupcompanyform-phone1');
	let signupPhoneResult = $('#signupform-phone');
	let signupPhoneResult2 = $('#signupcompanyform-phone');
	
	$('#signupform-country, #signupcompanyform-country').change(function(){
		let num = +($(this).val());
		let code = countryCodeSelect($(this).val());
//		$('#signupform-phone, #signupcompanyform-phone').val(code);
		countryCodeList.value = num;
		countryCodeList1.value = num;
		countryCodeList2.value = num;
		$('#countryCodeList option:contains('+code+')').prop('selected', true);

    countryCodeList1.val(num);
    countryCodeList1.trigger('change');
    countryCodeList2.val(num);
    countryCodeList2.trigger('change');

	});
	
	countryCodeList.change(function(){
		let code = countryCodeSelect($(this).val());
		let numb = $('#signup-phone-number').val();
		signupPhoneResult.val(code + numb);
	});
    countryCodeList1.change(function(){
		let code = countryCodeSelect($(this).val());
		let numb = $('#signup-phone-number').val();
		signupPhoneResult.val(code + numb);
	});
	signupPhoneNumber.keyup(function(){
		let code = countryCodeSelect($('#countryCodeList').val());
		let numb = this.value;
		signupPhoneResult.val(code + numb);
		$('#form-signup').yiiActiveForm('validateAttribute', 'signupform-phone');
	}) 
    countryCodeList2.change(function(){
		let code = countryCodeSelect($(this).val());
		let numb = $('#signupcompanyform-phone1').val();
		signupPhoneResult2.val(code + numb);
	});
	signupPhoneNumber2.keyup(function(){
		let code = countryCodeSelect($('#countryCodeList2').val());
		let numb = this.value;
		signupPhoneResult2.val(code + numb);
		$('#form-company-signup').yiiActiveForm('validateAttribute', 'signupcompanyform-phone');
	})
		
    function countryCodeSelect(coutry)
    {
        switch(coutry)
        {
            case '1': return '+(93)';
            case '2': return '+(358)';
